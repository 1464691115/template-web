{
  "editor.formatOnSave": true,
  "prettier.requireConfig": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "cSpell.words": [
    "hoverable",
    "Senuep"
  ],
  "path-intellisense.mappings": {
    "@config": "${workspaceRoot}/config/",
    "@public": "${workspaceRoot}/public/",
    "@": "${workspaceRoot}/src/",
    "@@": "${workspaceRoot}/src/",
  }
}
